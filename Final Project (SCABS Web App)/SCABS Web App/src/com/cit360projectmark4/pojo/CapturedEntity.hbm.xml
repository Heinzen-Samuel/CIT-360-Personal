<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.cit360projectmark4.pojo.CapturedEntity" table="captured" schema="scabs_webapp">
        <id name="capturedId">
            <column name="captured_id" sql-type="int(11)"/>
        </id>
        <property name="owner">
            <column name="owner" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="scab">
            <column name="scab" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="capturedStr">
            <column name="captured_str" sql-type="int(11)"/>
        </property>
        <property name="capturedLuck">
            <column name="captured_luck" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>