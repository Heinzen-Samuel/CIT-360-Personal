<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.cit360projectmark4.pojo.ScabsEntity" table="scabs" schema="scabs_webapp">
        <id name="scabId">
            <column name="scab_id" sql-type="int(11)"/>
        </id>
        <property name="scabName">
            <column name="scab_name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="defaultStr">
            <column name="default_str" sql-type="int(11)"/>
        </property>
        <property name="defaultLuck">
            <column name="default_luck" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>